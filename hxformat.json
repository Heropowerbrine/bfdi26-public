{
	"wrapping": {"maxLineLength": 200,"arrayWrap": {"defaultWrap": "keep"}},
	"lineEnds": {
		"anonFunctionCurly": {
		  "emptyCurly": "break",
		  "leftCurly": "after",
		  "rightCurly": "both"
		},
		"leftCurly": "both",
		"rightCurly": "both"
	  },
	"sameLine": {
		"functionBody": "same",
		"ifBody": "same",
		"ifElse": "next",
		"elseBody": "same",
		"doWhile": "next",
		"forBody": "same",
		"tryBody": "next",
		"tryCatch": "next"
	},
	"indentation":{
		"trailingWhitespace": true
	}
}
